package com.class9;

public class Patterns {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
for (int q=1;q<=6;q++) {
	for(int w=1;w<=5;w++) {
		if(w==2 || w==3||w==4||w==5) {
			System.out.println("\t");
		}
		for(int e=1;e<=5;e++) {
			for(int r=1;r<5;r++) {
				System.out.print("*");
			}
		}
	}
}
	}

}
