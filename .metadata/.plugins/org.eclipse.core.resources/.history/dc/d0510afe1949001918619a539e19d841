package com.class10;

public class TolyChallenge {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
for(int i=1;i<=7;i++) {
	for(int k=1;k<=7;k++) {
		if (i==1 && k==4) {
			System.out.print("*");
		
		}else {
			System.out.print(" ");
		}
	
		
		
	}
	System.out.println();
}
	}

}
