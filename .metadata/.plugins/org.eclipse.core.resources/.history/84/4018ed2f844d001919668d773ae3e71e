package com.class11;

public class AraysCountry2D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[][] cou = { { "Belarus", "Minsk" }, { "USA", "Washingron" }, { "Italy", "Rome" },
				{ "Canada", "Toronto" }, { "Austria", "Vena" }
		};
for(int i=0;i<cou.length;i++) {
	for(int q=0;q<cou.length;q++)
	System.out.println("The capital of "+cou[i][q] );
	
}
		
	}

}
