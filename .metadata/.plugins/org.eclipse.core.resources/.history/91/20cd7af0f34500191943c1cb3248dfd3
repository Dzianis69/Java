package ReviewClass;

public class Task3 {
	public static void main(String[] args) {
		/*
		 * for(int i=0;i<=30;i++)
		 *  { 
		 *  System.out.println("The time is:7."+i);
		 */

		/*
		 * int a=0; 
		 * while(a<=30) 
		 * {
		 *  System.out.println("The time is:7."+a); 
		 *  }
		 *  a++;
		 */

		int s = 0;
		do {
			System.out.println("The time is:7." + s);
			s++;
		} while (s <= 30);
	}
}
