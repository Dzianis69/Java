
public class MamaMilaRamy {

	public static String a, b, c;

	public static void mamaMila(String s) {

		a = "Mama";
		b = "Ramy";
		c = "Mila";
		s = a + "," + c + "," + b;
		System.out.println(s);

	}

	public static void Mama2(String s) {
		a="Mama";
		  b="Ramy";
		  c="Mila";
		  System.out.println( a + "," + c + "," + b);
	}

	public static void main(String[] args) {

		mamaMila(a);

	}
}
