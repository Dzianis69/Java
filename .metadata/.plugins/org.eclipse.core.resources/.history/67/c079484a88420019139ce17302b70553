package com.class7;

public class Increment {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int z =3;
		z++;//posticrement
		System.out.println(z);
		int v=10;
		v--;//postdecrement
		System.out.println(v);
		int a=20;
		int b=++a;
		int c=20;
		int d=c++;
		System.out.println();
		System.out.println(b);
		System.out.println(d);
		
	}

}
