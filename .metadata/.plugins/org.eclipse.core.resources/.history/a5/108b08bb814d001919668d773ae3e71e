package com.class11;

public class Task3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * Create an array on integers and calculate the sum of all elements in an
		 * array.
		 */
		int sum=0;
		int[] num=new int[10];
		for(int i=0;i<num.length;i++) {
			sum=sum+i;
		}
		System.out.println(sum);
	}

}
