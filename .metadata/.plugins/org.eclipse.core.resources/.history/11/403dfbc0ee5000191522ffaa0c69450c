package ReviewClass;

public class ArrayRow2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int [][] abc = {
		{2,6,9,3,7},
		{1,7,9,4,8},
		{0,4,6,2,5},
		{0,1,2,3}
		
};
int []ab=new int[1];

for(int i=0;i<abc.length;i++) {
	for(int q=0;q<abc[i].length;q++) {
		if(q==2){
	ab[0]+=abc[i][q];
		}
	}
}
System.out.println(ab[0]/abc[2].length);
	}

}
