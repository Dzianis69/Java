package com.class2;

public class Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double a, b;
		a = 3.14;
		b = 23.3;
		double sum = a + b;
		double sub = a - b;
		double div = a / b;
		double mult = a * b;
		System.out.println("The sum of 2 numbers " + a + " and " + b + " is equal to " + sum + "\n"
				+ "The sub of two numbers " + a + "and " + b + "is equal to " + sub + "\n" + "The div of two numbers "
				+ a + " and " + b + " is equal to " + div + "\n" + "The mult of two numbers " + a + " and " + b
				+ " is equal to " + mult + ".");

	}

}
