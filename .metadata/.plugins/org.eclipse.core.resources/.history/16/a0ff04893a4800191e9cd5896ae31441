package com.class9;

public class Task4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
/*Print the following pattern:
*
**
***
****
*****
****
***
**
*
*/
		
		for(int i=1;i<=9;i++) {
			for(int a=1;a<=i;a++) {
				if(a!=7||a!=8||a!=9) {
				System.out.print("*");
				}
			}
			System.out.println();
		}
	}

}
