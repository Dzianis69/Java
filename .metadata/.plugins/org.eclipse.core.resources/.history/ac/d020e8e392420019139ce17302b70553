package com.class7;

public class WhileLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=1;
		while(a<10) {
			System.out.println("Love");
			a++;
		}
System.out.println(a);

int b=0;
while(b<5) {
	System.out.println(b+" i am inside");
	b++;
}
System.out.println(b+" outside");
	}

}
