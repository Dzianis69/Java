package com.class16;

public class Car {

	String make, model, color;
	int door, wheels;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car car1 = new Car();
		car1.make="Honda";
		car1.model="Civic";
		car1.color="Black";
		car1.door=4;
		car1.wheels=4;
		
		
		Car car2=new Car();
		car2.make="Tesla";
		car2.model="x";
		car2.color="Blue";
		car2.door=4;
		car2.wheels=4;
		
		System.out.println("My car is "+car2.color+" "+car2.make+" "+car2.model);
		System.out.println(car1.wheels);
	}

}
